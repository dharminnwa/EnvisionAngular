{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular"
  },
  "apps": [
    {
      "root": "src",          
      "outDir": "../../Envision publish/dist",
      "assets": [
        "assets",
        "favicon.ico",
        "CompanyProfileDetail.html",
        "JS",
        "css"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "stylePreprocessorOptions": {
        "includePaths": [
          "app/@pages/styles",
          "pages_ui/scss"
        ]
      },
      "styles": [        
        "assets/plugins/bootstrap/css/bootstrap.min.css",
        "assets/plugins/codrops-stepsform/css/component.css",
        "assets/plugins/angular_tree_component/angular-tree-component.css",
        "../node_modules/ag-grid/dist/styles/ag-grid.css",
        "../node_modules/ag-grid/dist/styles/ag-theme-balham.css",
        "../node_modules/ag-grid/dist/styles/ag-theme-balham-dark.css",
        "../node_modules/ag-grid/dist/styles/ag-theme-blue.css",
        "../node_modules/ag-grid/dist/styles/ag-theme-bootstrap.css",
        "../node_modules/ag-grid/dist/styles/ag-theme-dark.css",
        "../node_modules/ag-grid/dist/styles/ag-theme-material.css",
        "../node_modules/ag-grid/dist/styles/compiled-icons.css",
        "../node_modules/ag-grid/dist/styles/theme-blue.css",
        "../node_modules/ag-grid/dist/styles/theme-bootstrap.css",
        "../node_modules/ag-grid/dist/styles/theme-dark.css",
        "../node_modules/ag-grid/dist/styles/theme-fresh.css",
        "../node_modules/ag-grid/dist/styles/theme-material.css",
        "assets/colorpicker/css/evol-colorpicker.min.css"
      ],
      "scripts": [
        "../node_modules/echarts/dist/echarts.min.js",
        "app/@pages/assets/javascript/pages.js",
        "assets/plugins/modernizr.custom.js",
        "assets/plugins/codrops-stepsform/js/classie.js",
        "assets/plugins/codrops-stepsform/js/stepsForm.js",
        "assets/plugins/skycons/skycons.js",        
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "assets/plugins/Infobox/infobox.js",
        "assets/plugins/MapContext/Gmap.js",
        "assets/colorpicker/js/evol-colorpicker.min.js",
        "../node_modules/crypto-js/crypto-js.js"      
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {},
    "build": {
      "showCircularDependencies": false
    } 
  }
}